<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pofol.main.imgProduct.domain.TestProductDto">
    <select id="selectAll" resultType="TestProductDto">
        SELECT * FROM protest
    </select>

    <select id="select" parameterType="int" resultType="TestProductDto">
        SELECT * FROM protest
        WHERE pro_id = #{pro_id}
    </select>
</mapper>

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pofol.main.product.category.CategoryDto">

    <!-- 카테고리 리스트 -->
    <select id="cateList" resultType="com.pofol.main.product.category.CategoryDto">
        SELECT cat_code, cat_name
        FROM category
        WHERE length(cat_code) = 3
        ORDER BY cat_code
    </select>




</mapper>